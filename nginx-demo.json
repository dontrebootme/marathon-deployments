{
  "id": "nginx-demo",
  "container": {
    "docker": {
      "image": "dontrebootme/nginx-demo:latest",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 0,
          "servicePort": 80,
          "protocol": "tcp"
        }
      ]
    },
    "type": "DOCKER"
  },
  "cpus": 0.5,
  "mem": 128.0,
  "instances": 4
}
